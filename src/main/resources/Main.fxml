                <TableColumn onEditCommit="#preConfigCommit" prefWidth="124.0" text="xpath" fx:id="xpathColumn" />
                <TableColumn fx:id="actionColumn" onEditCommit="#preConfigCommit" prefWidth="116.0" text="action" />
                <TableColumn fx:id="valueColumn" onEditCommit="#preConfigCommit" prefWidth="115.0" text="values" />
