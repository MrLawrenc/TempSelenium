<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.swust.controller.MainController">
    <children>
        <Button fx:id="openBtn" layoutX="33.0" layoutY="322.0" mnemonicParsing="false" onAction="#openBrowser"
                prefHeight="23.0" prefWidth="48.0" text="开启"/>
        <Button fx:id="closeBtn" layoutX="496.0" layoutY="322.0" mnemonicParsing="false" onAction="#quitBrowser"
                text="关闭"/>
        <Button fx:id="fullCheckBtn" layoutX="25.0" layoutY="354.0" mnemonicParsing="false" onAction="#fullCheckInfo"
                text="自动填充"/>
        <TextField fx:id="targetUrl" layoutX="95.0" layoutY="322.0" prefHeight="23.0" prefWidth="371.0"/>
        <Button fx:id="preCheckConfig" layoutX="14.0" layoutY="70.0" mnemonicParsing="false" text="核保前配置"/>
        <TableView fx:id="preCheckConfigList" editable="true" layoutX="95.0" layoutY="70.0" prefHeight="232.0"
                   prefWidth="466.0">
            <columns>
                <TableColumn prefWidth="124.0" text="xpath" fx:id="xpathColumn"/>
                <TableColumn fx:id="actionColumn" prefWidth="116.0" text="action"/>
                <TableColumn fx:id="valueColumn" prefWidth="115.0" text="value"/>
                <TableColumn fx:id="scriptColumn" prefWidth="110.0" text="script"/>
            </columns>
        </TableView>
        <ComboBox fx:id="companyIdBox" layoutX="95.0" layoutY="21.0" prefWidth="150.0"/>
        <ComboBox fx:id="productIdBox" layoutX="366.0" layoutY="21.0" prefWidth="150.0"/>
        <Text layoutX="25.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="companyId"/>
        <Text layoutX="300.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="productId"/>
    </children>
</AnchorPane>
