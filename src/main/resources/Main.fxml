<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.swust.controller.MainController">
    <children>
        <Button fx:id="openBtn" layoutX="33.0" layoutY="266.0" mnemonicParsing="false" onAction="#openBrowser" prefHeight="23.0" prefWidth="48.0" text="开启" />
        <Button fx:id="closeBtn" layoutX="485.0" layoutY="266.0" mnemonicParsing="false" onAction="#quitBrowser" text="关闭" />
        <Button fx:id="fullCheckBtn" layoutX="25.0" layoutY="310.0" mnemonicParsing="false" onAction="#fullCheckInfo" text="自动填充" />
        <TextField fx:id="targetUrl" layoutX="103.0" layoutY="266.0" prefHeight="23.0" prefWidth="371.0" />
        <Button fx:id="preCheckConfig" layoutX="19.0" layoutY="30.0" mnemonicParsing="false" text="核保前配置" />
        <TableView fx:id="preCheckConfigList" editable="true" layoutX="108.0" layoutY="20.0" prefHeight="232.0" prefWidth="466.0">
            <columns>
                <TableColumn prefWidth="124.0" text="xpath" fx:id="xpathColumn" />
                <TableColumn fx:id="actionColumn" prefWidth="116.0" text="action" />
                <TableColumn fx:id="valueColumn" prefWidth="115.0" text="value" />
                <TableColumn fx:id="scriptColumn" prefWidth="110.0" text="script" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
