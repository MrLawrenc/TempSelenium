            <font>
                <Font size="22.0" />
            </font>
        </Text>
        <Button layoutX="14.0" layoutY="334.0" mnemonicParsing="false" onAction="#newPage" text="新标签页" />
        <Button layoutX="296.0" layoutY="363.0" mnemonicParsing="false" onAction="#execConfig" text="执行样例" />
